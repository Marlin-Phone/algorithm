#include <bits/stdc++.h>
using namespace std;
/**
 * @nc app=nowcoder id=4bcf3081067a4d028f95acee3ddcd2b1 topic=295 question=701
 * lang=C++ 2025-08-25 23:38:33
 * https://www.nowcoder.com/practice/4bcf3081067a4d028f95acee3ddcd2b1?tpId=295&tqId=701
 * [BM55] 没有重复项数字的全排列
 */

/** @nc code=start */

class Solution {
  public:
    /**
     * 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
     *
     *
     * @param num int整型vector
     * @return int整型vector<vector<>>
     */
    int n;
    vector<int> arr;
    vector<vector<int>> res;
    vector<int> st;

    void dfs(int x, vector<int> &num) {
        if (x >= n) {
            res.push_back(arr);
            return;
        }
        for (int i = 0; i < n; i++) {
            if (st[i]) {
                continue;
            }
            st[i] = 1;
            arr[x] = num[i];
            dfs(x + 1, num);
            st[i] = 0;
            arr[x] = 0;
        }
    }
    vector<vector<int>> permute(vector<int> &num) {
        n = num.size();
        arr.resize(n);
        st.resize(n);
        dfs(0, num);
        return res;
    }
};

/** @nc code=end */
