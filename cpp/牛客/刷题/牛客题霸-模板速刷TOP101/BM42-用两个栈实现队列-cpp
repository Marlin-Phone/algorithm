#include <bits/stdc++.h>
using namespace std;
/**
 * @nc app=nowcoder id=54275ddae22f475981afa2244dd448c6 topic=295 question=23281
 * lang=C++ 2025-08-19 23:14:27
 * https://www.nowcoder.com/practice/54275ddae22f475981afa2244dd448c6?tpId=295&tqId=23281
 * [BM42] 用两个栈实现队列
 */

/** @nc code=start */

class Solution {
  public:
    void push(int node) { stack1.push(node); }

    int pop() {
        if (!stack2.empty()) {
            int temp = stack2.top();
            stack2.pop();
            return temp;
        }
        while (!stack1.empty()) {
            int temp = stack1.top();
            stack1.pop();
            stack2.push(temp);
        }
        int temp = stack2.top();
        stack2.pop();
        return temp;
    }

  private:
    stack<int> stack1;
    stack<int> stack2;
};

/** @nc code=end */
