#include <bits/stdc++.h>
using namespace std;
/**
 * @nc app=nowcoder id=50ec6a5b0e4e45348544348278cdcee5 topic=295
 * question=2188893 lang=C++ 2025-08-28 02:14:09
 * https://www.nowcoder.com/practice/50ec6a5b0e4e45348544348278cdcee5?tpId=295&tqId=2188893
 * [BM53] 缺失的第一个正整数
 */

/** @nc code=start */

class Solution {
  public:
    /**
     * 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
     *
     *
     * @param nums int整型vector
     * @return int整型
     */
    int minNumberDisappeared(vector<int> &nums) {
        int n = nums.size();
        for (int i = 0; i < n; i++) {
            if (nums[i] <= 0) {
                nums[i] = n + 1;
            }
        }
        for (int i = 0; i < n; i++) {
            int idx = abs(nums[i]) - 1;
            nums[idx] = -abs(nums[idx]);
        }
        for (int i = 0; i < n; i++) {
            cout << nums[i] << " ";
        }
        for (int i = 0; i < n; i++) {
            if (nums[i] > 0) {
                return i + 1;
            }
        }
        return n + 1;
    }
};

/** @nc code=end */
