#include <bits/stdc++.h>
using namespace std;

/**
 * @nc app=nowcoder id=37548e94a270412c8b9fb85643c8ccc2 topic=295 question=726
 * lang=C++ 2025-08-20 14:30:11
 * https://www.nowcoder.com/practice/37548e94a270412c8b9fb85643c8ccc2?tpId=295&tqId=726
 * [BM44] 有效括号序列
 */

/** @nc code=start */

class Solution {
  public:
    /**
     * 代码中的类名、方法名、参数名已经指定，请勿修改，直接返回方法规定的值即可
     *
     *
     * @param s string字符串
     * @return bool布尔型
     */
    bool isValid(string s) {
        stack<char> stk;
        for (auto it : s) {
            if (it == ']' || it == ')' || it == '}') {
                if (stk.empty()) {
                    return false;
                } else {
                    char temp = stk.top();
                    if ((temp == '[' && it == ']') ||
                        (temp == '(' && it == ')') ||
                        (temp == '{' && it == '}')) {
                        stk.pop();
                    } else {
                        return false;
                    }
                }
            } else {
                stk.push(it);
            }
        }
        if (stk.size()) {
            return false;
        }
        return true;
    }
};

/** @nc code=end */
